function Person() {
    this.firstName = "selvam";
    this.lastName = "selvi";
}

var person1 = new Person();
person1.firstName = "selvam";
person1.lastName = "selvi";
            
alert(person1.firstName + "for " + person1.lastName);

var person2 = new Person();
person2.firstName = "Bill";
person2.lastName = "Gates";
            
alert(person2.firstName + "for " + person2.lastName );